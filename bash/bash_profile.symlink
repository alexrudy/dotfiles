# Alex Rudy's Bash Profile
# 2010-09-10
# This file contains cool stuff for the terminal area, but not useful without a terminal window open.

if [ -f "$HOME/.bashrc" ] ; then
	. $HOME/.bashrc
fi

if [ -f "$HOME/.dircolors" ] ; then
  eval $(/opt/bin/dircolors -b $HOME/.dircolors)
fi


PATH="${PATH}:/Library/Frameworks/Python.framework/Versions/2.6/bin"
export PATH
