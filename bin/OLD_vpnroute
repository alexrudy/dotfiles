#!/bin/bash####login administratorDFGW=`netstat -ra | grep default | awk '{print $2}'`POMVPNGW="134.173.70.2"if [ -e ~/.bin/orgGW ]then	DFGWorg=`cat ~/.bin/orgGW`	sudo route delete default	sudo route add default $DFGWorg	rm ~/.bin/orgGWelse	# save org defalut GW to file	echo $DFGW > ~/.bin/orgGW	echo $DFGW	echo 	echo 	sudo route delete default	sudo route add default $POMVPNGWfi